{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "fly-io",
  "title": "Fly.io",
  "description": "explore apps managed on Fly.io",
  "icon": "flyio.png",
  "author": "nagauta",
  "categories": [
    "Developer Tools",
    "Web",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-apps",
      "title": "Search Apps",
      "subtitle": "Fly.io",
      "description": "search Apps on Fly.io",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "flyioApiKey",
      "type": "password",
      "title": "Flyio API Key",
      "required": true,
      "description": "type your Flyio api key for auth",
      "placeholder": "xxxxxxxxxx",
      "label": "Flyio api key"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.58.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}